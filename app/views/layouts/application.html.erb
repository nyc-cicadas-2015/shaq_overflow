<!DOCTYPE html>
<html>
<head>
  <title>ShaqOverflow</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
	<%= render partial: "layouts/header" %>
	<% flash.each do |name, msg| %>
	  <%= content_tag :div, msg, class: name %>
	<% end %>
	<%= yield %>
	</div>
</body>
</html>
